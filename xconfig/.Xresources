! ------------------------------------------------------------------------------
! URxvt configuration
! ------------------------------------------------------------------------------
URxvt*geometry:  186x51
URxvt*loginShell:  true
URxvt*scrollTtyKeypress:  true
URxvt*skipScroll:  true
URxvt*scrollBar:  false
! Perl extension
URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select,resize-font
URxvt.url-select.launcher:  /bin/xdg-open
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

! Background Transparency
!URxvt*inheritPixmap: true
! Fonts
URxvt*font: xft:Ubuntu Mono:size=14:style=regular,xft:Source Han Sans JP:size=14
URxvt*boldFont: xft:Ubuntu Mono:Bold

URxvt*italicFont: xft:Ubuntu Mono:Italic
URxvt*boldItalicFont: xft:Ubuntu Mono:Bold
